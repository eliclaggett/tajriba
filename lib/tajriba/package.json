{
  "name": "@empirica/tajriba",
  "version": "1.0.0-alpha.3",
  "description": "Tajriba client",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empiricaly/tajriba.git"
  },
  "bugs": {
    "url": "https://github.com/empiricaly/tajriba/issues"
  },
  "homepage": "https://github.com/empiricaly/tajriba#readme",
  "keywords": [
    "tajriba",
    "experiment",
    "research",
    "multiplayer",
    "real-time",
    "behavioral",
    "social-science",
    "psychology"
  ],
  "author": "Nicolas Paton <nicolas.paton@gmail.com>",
  "license": "Apache-2.0",
  "files": [
    "*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "volta": {
    "node": "16.4.1",
    "yarn": "1.22.10",
    "npm": "7.19.1"
  },
  "np": {
    "contents": "pkg"
  },
  "scripts": {
    "build": "pika build",
    "generate": "graphql-codegen",
    "generate:watch": "graphql-codegen -w",
    "prerelease": "yarn run build",
    "release:next": "np --tag next --contents pkg/",
    "release": "np --tag latest --contents pkg/",
    "postversion": "cd pkg && yarn version --new-version $npm_package_version --no-git-tag-version --no-commit-hooks"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "pika-plugin-package.json",
        {
          "+volta": "^",
          "+author": "^",
          "-devDependencies": {}
        }
      ],
      [
        "@pika/plugin-build-types"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-deno"
      ]
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typed-document-node": "1.18.2",
    "@graphql-codegen/typescript": "1.20.2",
    "@graphql-codegen/typescript-apollo-client-helpers": "1.1.2",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-typed-document-node/core": "3.1.0",
    "@pika/pack": "0.5.0",
    "@pika/plugin-build-deno": "0.9.2",
    "@pika/plugin-build-node": "0.9.2",
    "@pika/plugin-build-types": "0.9.2",
    "@pika/plugin-build-web": "0.9.2",
    "@pika/plugin-ts-standard-pkg": "0.9.2",
    "np": "7.5.0",
    "pika-plugin-package.json": "1.0.2",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@apollo/client": "3.3.7",
    "graphql": "15.5.0",
    "graphql-ws": "^5.3.0",
    "isomorphic-ws": "^4.0.1",
    "subscriptions-transport-ws": "0.9.18",
    "websocket": "1.0.33"
  }
}
